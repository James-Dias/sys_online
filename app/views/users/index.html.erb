<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Rg</th>
      <th>Address</th>
      <th>Phone1</th>
      <th>Phone2</th>
      <th>Kind</th>
      <th>Image</th>
      <th>Nickname</th>
      <th>Neighborhood</th>
      <th>City</th>
      <th>Zone</th>
      <th>Occupation</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.cpf %></td>
        <td><%= user.rg %></td>
        <td><%= user.address %></td>
        <td><%= user.phone1 %></td>
        <td><%= user.phone2 %></td>
        <td><%= user.kind %></td>
        <td><%= user.image %></td>
        <td><%= user.nickname %></td>
        <td><%= user.neighborhood %></td>
        <td><%= user.city %></td>
        <td><%= user.zone %></td>
        <td><%= user.occupation %></td>
        <td><%= user.description %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
